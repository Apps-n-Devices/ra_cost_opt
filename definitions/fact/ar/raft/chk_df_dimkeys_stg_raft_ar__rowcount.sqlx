config {
    type: "assertion",
    dependencies: [
        "df_dimkeys_stg_raft_ar"    
    ]
}

SELECT
  *
FROM (
  SELECT (
    SELECT COUNT(*)
    FROM readanalytics.df_dimkeys_stg_raft_ar
  ) AS dimkeys_count,
  (SELECT COUNT(*)
    FROM readanalytics.df_dedup_stg_raft_ar
  ) AS dedup_count
) 
WHERE
  dimkeys_count != dedup_count